{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "indent": [
            true,
            "spaces"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "no-var-requires": false,
        "object-literal-key-quotes": false,
        "member-ordering": [
            false
        ],
        "object-literal-sort-keys": false,
        "jsx-alignment": [
            false
        ],
        "jsx-wrap-multiline": true
    }
}